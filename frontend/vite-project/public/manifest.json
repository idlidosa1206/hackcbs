{
    "name": "SpecTRE",
    "description": "Vulnerebility analysis",
    "version": "0.0.1",
    "manifest_version": 3,
    "action": {
      "default_popup": "index.html"
    },
    "permissions": [
      "activeTab",
      "storage"
    ],
    "host_permissions": [
      "http://127.0.0.1:4444/*"
    ],
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'; connect-src 'self' http://127.0.0.1:4444;"
    }
  }